{
	"manifest_version": 2,
	"name": "Blackboard Fix",
	"description": "Cause Learn.UQ should link to learn.uq.edu.au not elearning.uq.edu.au",
	"version": "0.1",
	"content_scripts": [{
		"matches": ["*://learn.uq.edu.au/*"],
		"js": ["content.js"]
	}],
    "icons": { 
        "128": "fix_blackboard.iconset/icon_16x16.png",
        "128": "fix_blackboard.iconset/icon_32x32.png",
        "128": "fix_blackboard.iconset/icon_128x128.png",
        "128": "fix_blackboard.iconset/icon_256x256.png",
        "128": "fix_blackboard.iconset/icon_512x512.png" 
    }
}
